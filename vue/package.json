{
  "name": "@boxyhq/vue-ui",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --declaration --emitDeclarationOnly --declarationDir dist/ --rootDir src -p tsconfig.app.json --composite false",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "files": [
    "dist",
    "index.d.ts",
    "sso.d.ts"
  ],
  "types": "./index.d.ts",
  "exports": {
    "./sso": {
      "import": "./dist/sso.js",
      "require": "./dist/sso.umd.cjs"
    }
  },
  "devDependencies": {
    "@tsconfig/node18": "2.0.1",
    "@types/node": "20.2.3",
    "@vitejs/plugin-vue": "4.2.3",
    "@vue/tsconfig": "0.4.0",
    "npm-run-all": "4.1.5",
    "typescript": "5.0.4",
    "vite": "4.3.8",
    "vite-plugin-css-injected-by-js": "3.1.1",
    "vitepress": "1.0.0-beta.1",
    "vue": "3.3.4",
    "vue-tsc": "1.6.5"
  },
  "peerDependencies": {
    "vue": "^3.3.4 || ^2.7.x"
  }
}
